import database.java
import java.util.Date;


public class Report {
    private int BudgetDistribution;
    private String ProductDisstribution;
    private int TotalApplicationsRecived;
    private int ApplicationsApproved;
    private double TotalCashDonation;
    private int TotalGoodsDonation;
    private String Period   ;
    private float ApprovalRate;
        
        
     /**
     * Default constructor
     */
    public Report(String p) {
        this.Period = p;
        this.BudgetDistribution = DatabaseArray.getBudgetDistribution(p);
        this.ProductDisstribution = DatabaseArray.getProductDistribution(p);
        this.TotalApplicationsReceived = DatabaseArray.getTotalApplicationsReceived(p);
        this.ApplicationsApproved = DatabaseArray.getApplicationsApproved(p);
        this.TotalCashDonation = DatabaseArray.getTotalCashDonation(p);
        this.TotalGoodsDonation = DatabaseArray.getTotalGoodsDonationt(p);
        this.ApprovalRate = DatabaseArray.CalculateApprovalRate();
        
        }     
        
