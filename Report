import database.java
import java.util.Date;


public class Report {
    private int BudgetDistribution;
    private String ProductDisstribution;
    private int TotalApplicationsRecived;
    private int ApplicationsApproved;
    private double TotalCashDonation;
    private int TotalGoodsDonation;
    private periodical Periodial   ;
    private float ApprovalRate;
        
        
     /**
     * Default constructor
     */
    public Report() {
        this.BudgetDistribution = database.getBudget;
        this.ProductDisstribution = database.TotalProductDistributiontoTheCurrentDate;
        this.ApplicationsApproved = database.ApplicationsApprovedtoTheCurrentDate;
        this.TotalCashDonation = database.TotalCashDonationtoTheCurrentDate;
        this.TotalGoodsDonation = database.TotalGoodsDonationtoTheCurrentDate;
        this.ApprovalRate = database.CalculateApprovalRate;
        
        }     
        
    
        
        
 } 
    public void printReport();
        System.out.println("Budget Distribution:" + BudgetDistribution )
        System.out.println("Product Distribution:" + ProductDistribution )
        System.out.println("Total Applications Received:" + Product Distribution)
        System.out.println("Applications Approved:" + Applications)
        System.out.println("Total Cash Donation:" + TotalCashDonation)
        System.out.println("Total Goods Donation:" + TotalGoodsDonation)
        System.out.println("Approval Rate:" + ApprovalRate)
}
